{
    "extends": [
        "config:base",
        "schedule:weekly",
        ":disableDependencyDashboard"
    ],
    "semanticCommits": false,
    "postUpdateOptions": [
        "gomodTidy",
        "gomodNoMassage"
    ],
    "commitMessagePrefix": "chore(all): ",
    "commitMessageAction": "update",
    "groupName": "all",
    "ignoreDeps": [
        "google.golang.org/genproto"
    ],
    "ignorePaths": [
        "**/snippets/**"
    ],
    "force": {
        "constraints": {
            "go": "1.19"
        }
    }
}
